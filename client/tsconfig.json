{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2017"],
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components/index"],
      "@pages": ["src/pages/index"],
      "@validators": ["src/validators/index"],
      "@utils": ["src/utils/index"]
    },
    "typeRoots": ["./node_modules/@types", "./src/types"],
  },
}
